page{
	background-color: #FFFFFF;
}
.tip{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	padding-left: 2%;
	padding-right: 2%;
	font-size: 32rpx;
	line-height: 64rpx;
	font-weight: 400;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.tip .total .out{
	padding-right: 20rpx;
}
.detailList{
	margin-left: 2%;
	margin-right: 2%;
	border-top: 8rpx solid #999999;
	padding-top: 20rpx;
}
.detailList .listItem{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	border-bottom: 2rpx dashed #999999;
	margin-bottom: 20rpx;
}
.detailList .listItem .typeIcon{
	width: 12%;
}
.detailList .listItem .typeIcon image{
	width: 60rpx;
	height: 60rpx;
}
.detailList .listItem .wrapper{
	width: 88%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	font-size: 32rpx;
	line-height: 60rpx;
}

